{
	"subs": {
		"match": "^(?P<name>.*)\\s+EP(?P<episode>\\d+).*(?=\\.)(?P<extension>.*)$",
		"replace": "\\g<name> - Episode \\g<episode>\\g<extension>",
		"ext": ["*.srt", "*.ass"],
		"whitelist": true
	},
	"anidl": {
		"match": "\\[AniDL\\]\\[wat15\\](?P<name>.*)(?=\\s+-)\\s?-\\s?[0]?(?P<episode>\\d+).*(?=\\.)(?P<extension>.*)$",
		"replace": "\\g<name> - Episode \\g<episode>\\g<extension>",
		"ext": ["*.mkv", "*.mp4"]
	},
	"_2s": {
		"match": "[_]+",
		"replace": " ",
		"ext": ["*"]
	}
}